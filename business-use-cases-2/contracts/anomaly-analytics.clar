(define-map anomaly_analytics_registry uint {creator: principal, data: (string-ascii 256), timestamp: uint})
(define-data-var anomaly_analytics_nonce uint u0)
(define-read-only (get-entry (id uint)) (map-get? anomaly_analytics_registry id))
(define-public (register (data (string-ascii 256)))
  (let ((id (+ (var-get anomaly_analytics_nonce) u1)))
    (map-set anomaly_analytics_registry id {creator: tx-sender, data: data, timestamp: stacks-block-height})
    (var-set anomaly_analytics_nonce id)
    (ok id)))
(define-public (update-entry (id uint) (data (string-ascii 256)))
  (let ((entry (unwrap! (map-get? anomaly_analytics_registry id) (err u1))))
    (asserts! (is-eq tx-sender (get creator entry)) (err u2))
    (map-set anomaly_analytics_registry id (merge entry {data: data}))
    (ok true)))
