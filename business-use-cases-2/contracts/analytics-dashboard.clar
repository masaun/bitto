(define-map analytics_dashboard_registry uint {creator: principal, data: (string-ascii 256), timestamp: uint})
(define-data-var analytics_dashboard_nonce uint u0)
(define-read-only (get-entry (id uint)) (map-get? analytics_dashboard_registry id))
(define-public (register (data (string-ascii 256)))
  (let ((id (+ (var-get analytics_dashboard_nonce) u1)))
    (map-set analytics_dashboard_registry id {creator: tx-sender, data: data, timestamp: stacks-block-height})
    (var-set analytics_dashboard_nonce id)
    (ok id)))
(define-public (update-entry (id uint) (data (string-ascii 256)))
  (let ((entry (unwrap! (map-get? analytics_dashboard_registry id) (err u1))))
    (asserts! (is-eq tx-sender (get creator entry)) (err u2))
    (map-set analytics_dashboard_registry id (merge entry {data: data}))
    (ok true)))
