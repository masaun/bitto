# Chainhooks Configuration
# Copy this file to .env.local and fill in your API key

# Hiro API Key for Chainhooks SDK (optional but recommended)
# Get your API key from https://platform.hiro.so/
REACT_APP_HIRO_API_KEY=your_hiro_api_key_here

# Chainhook Server Configuration (for local development)
REACT_APP_CHAINHOOK_BASE_URL=http://localhost:20456
REACT_APP_CHAINHOOK_AUTH_TOKEN=fungible-token-webhook

# Network Configuration
REACT_APP_NETWORK=mainnet
#REACT_APP_NETWORK=testnet